package org.talend.survivorship.sample 

rule "STG_VAT"
    no-loop true
    dialect "mvel"
    ruleflow-group "STG_VATGroup"
when
    $input : RecordIn( dataset.isMostComplete( $input.TALEND_INTERNAL_ID ) )  
then
    dataset.survive( $input.TALEND_INTERNAL_ID, "STG_VAT", drools.getRule().getName());

end


